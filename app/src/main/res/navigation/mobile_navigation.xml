<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/navigation_splash">
    <fragment
        android:id="@+id/navigation_splash"
        android:name="com.example.bembosapporiginal.ui.Splash"
        android:label="fragment_splash"
        tools:layout="@layout/fragment_splash" >
        <action
            android:id="@+id/action_splash_to_bienvenida1"
            app:destination="@id/navigation_bienvenida1" />
        <action
            android:id="@+id/action_navigation_splash_to_bienvenida1"
            app:destination="@id/navigation_bienvenida1" />
    </fragment>
    <fragment
        android:id="@+id/navigation_bienvenida2"
        android:name="com.example.bembosapporiginal.ui.Bienvenida2"
        android:label="fragment_bienvenida2"
        tools:layout="@layout/fragment_bienvenida2" >
        <action
            android:id="@+id/action_bienvenida2_to_login"
            app:destination="@id/navigation_login" />
    </fragment>
    <fragment
        android:id="@+id/navigation_login"
        android:name="com.example.bembosapporiginal.ui.Login"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_login_to_loginGoogle"
            app:destination="@id/navigation_loginGoogle" />
        <action
            android:id="@+id/action_navigation_login_to_navigation_registrar3"
            app:destination="@id/navigation_registrar" />
        <action
            android:id="@+id/action_navigation_login_to_navigation_recuperarContra"
            app:destination="@id/navigation_recuperarContra" />
    </fragment>
    <fragment
        android:id="@+id/navigation_loginGoogle"
        android:name="com.example.bembosapporiginal.ui.LoginGoogle"
        android:label="fragment_login_google"
        tools:layout="@layout/fragment_login_google" />
    <fragment
        android:id="@+id/navigation_bienvenida1"
        android:name="com.example.bembosapporiginal.ui.Bienvenida1"
        android:label="fragment_bienvenida1"
        tools:layout="@layout/fragment_bienvenida1" >
        <action
            android:id="@+id/action_bienvenida1_to_navigation_bienvenida2"
            app:destination="@id/navigation_bienvenida2" />
    </fragment>
    <fragment
        android:id="@+id/navigation_registrar"
        android:name="com.example.bembosapporiginal.ui.Registrar"
        android:label="fragment_registrar"
        tools:layout="@layout/fragment_registrar" />
    <fragment
        android:id="@+id/navigation_recuperarContra"
        android:name="com.example.bembosapporiginal.ui.RecuperarContra"
        android:label="fragment_recuperar_contra"
        tools:layout="@layout/fragment_recuperar_contra" >
        <action
            android:id="@+id/action_navigation_recuperarContra_to_restablecerContra"
            app:destination="@id/restablecerContra" />
    </fragment>
    <fragment
        android:id="@+id/restablecerContra"
        android:name="com.example.bembosapporiginal.ui.RestablecerContra"
        android:label="fragment_restablecer_contra"
        tools:layout="@layout/fragment_restablecer_contra" >
        <action
            android:id="@+id/action_restablecerContra_to_confirmContra"
            app:destination="@id/confirmContra" />
    </fragment>
    <fragment
        android:id="@+id/confirmContra"
        android:name="com.example.bembosapporiginal.ui.ConfirmContra"
        android:label="fragment_confirm_contra"
        tools:layout="@layout/fragment_confirm_contra" />
</navigation>